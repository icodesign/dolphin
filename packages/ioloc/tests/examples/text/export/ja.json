{
  "name": "xliff",
  "type": "element",
  "attributes": {
    "version": "2.0",
    "srcLang": "en",
    "trgLang": "ja"
  },
  "elements": [
    {
      "name": "file",
      "type": "element",
      "attributes": {
        "id": "${ID}",
        "original": "${ORIGINAL_PATH}"
      },
      "elements": [
        {
          "name": "unit",
          "type": "element",
          "attributes": {
            "id": "${ID}"
          },
          "elements": [
            {
              "name": "segment",
              "type": "element",
              "attributes": {
                "state": "initial"
              },
              "elements": [
                {
                  "name": "source",
                  "type": "element",
                  "elements": [
                    {
                      "type": "text",
                      "text": "**Dolphin** is an innovative, automated internationalization (i18n) service designed to integrate seamlessly into your development pipeline, with the power of AI.\n\n<pc id=\"name\" ctype=\"x-python-brace-param\">{name}</pc>\n"
                    }
                  ]
                },
                {
                  "name": "target",
                  "type": "element",
                  "elements": [
                    {
                      "type": "text",
                      "text": ""
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
